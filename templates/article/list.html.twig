{% extends 'base.html.twig' %}

{% block title %}Liste des articles{% endblock %}

{% block body %}
    <h1>Liste des articles</h1>

  
        <ul>
            {% for article in articles %}
                <li>
                    <h3>{{ article.title }}</h3>
                    <p>{{ article.content}}</p>

                    <p>{{ article.date_de_publication }}</p>
                   
                </li>
            {% endfor %}
        </ul>
   
{% endblock %}
